let x0 = 3.14;    // A number with decimals
let y = 3;
let x1 = 123e5;    // 12300000
let y2 = 123e-5;   // 0.00123
let y3 = (0.2*10 + 0.1*10) / 10;
let z = x + y;
x2.toString();
(123).toString();
(100 + 23).toString(); //retorna número como uma string
let x = 9.656;
x.toExponential(2); // retorna uma string, com um número arredondado e escrito usando notação exponencial.
x.toExponential(4); // 9.6560e+0
x.toExponential(6); //9.656000e+0
let x3 = 9.656;
x3.toFixed(0); // retorna uma string, com o número escrito com um número especificado de decimais:
x3.toFixed(2);
x3.toPrecision() // retorna uma string, com um número escrito com um comprimento especificado:
Number() //pode ser usado para converter variáveis ​​JavaScript em números
parseInt() //analisa uma string e retorna um número inteiro. Espaços são permitidos. Apenas o primeiro número é retornado
parseFloat() //analisa uma string e retorna um número. Espaços são permitidos. Apenas o primeiro número é retornado
let x4 = Number.MAX_VALUE; // retorna o maior número possível em JavaScript.
let x5 = Number.MIN_VALUE; // retorna o menor número possível em JavaScript.